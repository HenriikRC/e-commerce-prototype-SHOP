<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SHOP.Shop.PHCategoryDataController">
   <children>
      <ImageView fx:id="categoryImage" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="categoryButton" mnemonicParsing="false" style="-fx-background-color: #2D728F; -fx-background-radius: 10px;" textFill="WHITE" VBox.vgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="25.0" left="5.0" right="5.0" />
   </padding>
</VBox>
